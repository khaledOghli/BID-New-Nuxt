{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.0.5",
  "scripts": {
    "build": "nuxi build",
    "dev:pwa": "VITE_PLUGIN_PWA=true nuxi dev",
    "dev:docs": "cd docs && pnpm run dev",
    "dev": "nuxi dev",
    "generate": "nuxi generate",
    "prepare": "husky",
    "start": "node .output/server/index.mjs",
    "start:generate": "npx serve .output/public",
    "lint": "eslint . --ignore-pattern 'theme/' --ignore-pattern 'node_modules/'  --ignore-pattern 'assets/primevue'",
    "lint:fix": "eslint --ignore-pattern 'theme/' --ignore-pattern 'node_modules/' --ignore-pattern 'assets/primevue --fix",
    "format": "prettier --write .",
    "lint-staged": "lint-staged",
    "commit": "lint-staged && git-cz",
    "typecheck": "vue-tsc --noEmit",
    "generate-theme-light": "sass --update assets/primevue/themes/aura/light/blue/theme.scss:public/themes/aura-light-blue/theme.css",
    "generate-theme-dark": "sass --update assets/primevue/themes/aura/dark/blue/theme.scss:public/themes/aura-dark-blue/theme.css",
    "generate-themes": "pnpm run generate-theme-light && pnpm run generate-theme-dark",
    "create:app": "node createNuxtApp.js"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.8.2",
    "highlight.js": "^11.9.0",
    "markdown-it": "^14.1.0",
    "markdown-it-prism": "^2.3.0",
    "nuxt-icons": "^3.2.1",
    "nuxt-umami": "^2.6.1",
    "primevue": "3.52.0",
    "vee-validate": "^4.12.8",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.18.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@commitlint/prompt-cli": "^19.3.1",
    "@dargmuesli/nuxt-cookie-control": "^8.3.2",
    "@iconify-json/carbon": "^1.1.33",
    "@iconify-json/twemoji": "^1.1.15",
    "@nuxt/content": "^2.12.1",
    "@nuxt/eslint": "^0.3.12",
    "@nuxt/fonts": "^0.7.0",
    "@nuxt/image": "^1.7.0",
    "@nuxt/scripts": "^0.4.1",
    "@nuxt/types": "^2.17.3",
    "@nuxthq/studio": "^1.0.15",
    "@nuxtjs/color-mode": "^3.4.1",
    "@nuxtjs/fontaine": "^0.4.1",
    "@nuxtjs/i18n": "^8.3.1",
    "@nuxtjs/storybook": "^8.0.0",
    "@nuxtjs/web-vitals": "^0.2.7",
    "@pinia/nuxt": "^0.5.1",
    "@unlazy/nuxt": "^0.11.3",
    "@unocss/eslint-config": "^0.60.2",
    "@unocss/nuxt": "^0.60.2",
    "@unocss/preset-wind": "^0.60.2",
    "@vite-pwa/nuxt": "^0.7.0",
    "@vueuse/nuxt": "^10.9.0",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "commitizen": "^4.3.0",
    "consola": "^3.2.3",
    "conventional-changelog-atom": "^5.0.0",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-format": "^0.1.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-spellcheck": "^0.0.20",
    "husky": "^9.0.11",
    "inquirer": "9",
    "lint-staged": "^15.2.2",
    "lodash": "^4.17.21",
    "nuxt": "^3.11.2",
    "nuxt-icon": "^0.6.10",
    "nuxt-lodash": "^2.5.3",
    "nuxt-primevue": "^0.3.1",
    "nuxt-security": "2.0.0-beta.5",
    "nuxt-time": "^0.1.3",
    "nuxt-viewport": "^2.1.5",
    "pinia": "^2.1.7",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "sass": "^1.77.1",
    "typescript": "^5.4.5",
    "unocss": "^0.60.2",
    "unocss-preset-scrollbar": "^0.3.1",
    "volar-service-prettier": "^0.0.45",
    "volar-service-prettyhtml": "^0.0.45",
    "vue-eslint-parser": "^9.4.2",
    "vue-i18n": "9",
    "vue-loader": "^17.4.2",
    "vue-tsc": "^2.0.19"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
