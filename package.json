{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.1.1",
  "scripts": {
    "build": "nuxi build",
    "install": "node install.mjs",
    "dev:pwa": "VITE_PLUGIN_PWA=true nuxi dev",
    "dev:docs": "cd docs && pnpm run dev",
    "dev": "nuxi dev",
    "generate": "nuxi generate",
    "prepare": "husky",
    "start": "node .output/server/index.mjs",
    "start:generate": "npx serve .output/public",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "lint-staged": "lint-staged",
    "commit": "lint-staged && git-cz",
    "typecheck": "vue-tsc --noEmit",
    "generate-theme-light": "sass --update assets/primevue/themes/aura/light/blue/theme.scss:public/themes/aura-light-blue/theme.css",
    "generate-theme-dark": "sass --update assets/primevue/themes/aura/dark/blue/theme.scss:public/themes/aura-dark-blue/theme.css",
    "generate-themes": "pnpm run generate-theme-light && pnpm run generate-theme-dark",
    "create:app": "node createNuxtApp.js",
    "storybook": "storybook dev -p 6006"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.8.2",
    "@headlessui/vue": "^1.7.22",
    "@unocss/runtime": "^0.60.4",
    "@vee-validate/zod": "^4.13.0",
    "highlight.js": "^11.9.0",
    "lottie-web": "^5.12.2",
    "markdown-it": "^14.1.0",
    "markdown-it-prism": "^2.3.0",
    "nuxt-icons": "^3.2.1",
    "nuxt-umami": "^2.6.2",
    "primevue": "3.52.0",
    "sweetalert2": "^11.11.1",
    "unimport": "^3.7.2",
    "vee-validate": "^4.13.0",
    "yup": "^1.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.20.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@commitlint/prompt-cli": "^19.3.1",
    "@dargmuesli/nuxt-cookie-control": "^8.4.1",
    "@eslint/js": "^9.4.0",
    "@iconify-json/carbon": "^1.1.34",
    "@iconify-json/twemoji": "^1.1.15",
    "@nuxt/content": "^2.12.1",
    "@nuxt/eslint": "^0.3.13",
