{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.0.5",
  "scripts": {
    "build": "nuxi build",
    "dev:pwa": "VITE_PLUGIN_PWA=true nuxi dev",
    "dev": "nuxi dev",
    "generate": "nuxi generate",
    "prepare": "husky",
    "start": "node .output/server/index.mjs",
    "start:generate": "npx serve .output/public",
    "lint": "eslint ./app.vue",
    "lint:fix": "eslint . --fix",
    "commit": "commit",
    "typecheck": "vue-tsc --noEmit",
    "generate-theme-light": "sass --update assets/primevue/themes/aura/light/blue/theme.scss:public/themes/aura-light-blue/theme.css",
    "generate-theme-dark": "sass --update assets/primevue/themes/aura/dark/blue/theme.scss:public/themes/aura-dark-blue/theme.css",
    "generate-themes": "pnpm run generate-theme-light && pnpm run generate-theme-dark"
  },
  "dependencies": {
    "highlight.js": "^11.9.0",
    "markdown-it": "^14.1.0",
    "markdown-it-prism": "^2.3.0",
    "primevue": "3.52.0",
    "vee-validate": "^4.12.8",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.17.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/prompt-cli": "^19.3.0",
    "@iconify-json/carbon": "^1.1.32",
    "@iconify-json/twemoji": "^1.1.15",
    "@nuxt/content": "^2.12.1",
    "@nuxt/eslint": "^0.3.12",
    "@nuxtjs/color-mode": "^3.4.1",
    "@nuxtjs/fontaine": "^0.4.1",
    "@nuxtjs/i18n": "^8.3.1",
    "@pinia/nuxt": "^0.5.1",
    "@unocss/eslint-config": "^0.60.2",
    "@unocss/nuxt": "^0.60.2",
    "@vite-pwa/nuxt": "^0.7.0",
    "@vueuse/nuxt": "^10.9.0",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "consola": "^3.2.3",
    "eslint": "^9.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-format": "^0.1.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "nuxt": "^3.11.2",
    "nuxt-icon": "^0.6.10",
    "nuxt-primevue": "^0.3.1",
    "pinia": "^2.1.7",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "sass": "^1.77.1",
    "typescript": "^5.4.5",
    "unocss": "^0.60.2",
    "unocss-preset-scrollbar": "^0.3.1",
    "vue-loader": "^17.4.2",
    "vue-tsc": "^2.0.17"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
